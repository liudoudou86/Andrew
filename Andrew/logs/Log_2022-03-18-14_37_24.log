    <script type="text/javascript">

        $().ready(function() {
            // 在键盘按下并释放及提交后验证提交表单
            $("#myForm").validate({
                rules: {
                    username: "required",
                    password: "required"
                },
                messages: {
                    username: "请输入用户名",
                    password: "请输入密码"
                },
                errorPlacement:function(error, element) {
//                    error.appendTo(element.parent());//Default
//                    error.appendTo(element.parent("div").next("div"));

                    if (element.attr("name") == "username") {
                        console.log("name");
                        error.insertAfter("#err-user-msg");
                    } else if(element.attr("name") == "password"){
                        console.log("password");
                        error.insertAfter("#err-pwd-msg");
                    }else{
                        console.log("else");
                        error.insertAfter(element);
                    }


//                    error.appendTo(element.parent("td").next("td"));

                }
            });
        });
        function forgetPwd() {
            B2B.bsdialog.alert( '请将用户名或公司名称及接收邮箱发送至\n微信公众号(大健康life)后台\n相关人员会将新密码发送至您的邮箱' );
        }
    </script>

